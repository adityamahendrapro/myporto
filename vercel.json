{
  "version": 2,
  "builds": [
    {
      "src": "/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "/assets/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "/CV_Aditya Mahendra.pdf",
      "use": "@vercel/static"
    },
    {
      "src": "src/img/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
